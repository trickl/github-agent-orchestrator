# GitHub Agent Orchestrator (Phase 1/1A)
# Copy this file to .env and fill in values.

# GitHub authentication
ORCHESTRATOR_GITHUB_TOKEN=ghp_your_token_here
GITHUB_BASE_URL=https://api.github.com

# Logging
LOG_LEVEL=INFO

# Local persistence
AGENT_STATE_PATH=agent_state

# REST server (optional)
# The server needs a default repo for PR monitoring because Phase 1 state does not
# yet store per-issue repository.
ORCHESTRATOR_DEFAULT_REPO=owner/repo
ORCHESTRATOR_HOST=127.0.0.1
ORCHESTRATOR_PORT=8000
